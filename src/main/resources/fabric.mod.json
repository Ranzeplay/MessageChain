{
  "schemaVersion": 1,
  "id": "messagechain",
  "version": "${version}",
  "name": "MessageChain",
  "description": "A Minecraft fabric mod allows developers to interact with server resources like calling web API.",
  "authors": ["Jeb Feng"],
  "contact": {
    "repo": "https://github.com/Ranzeplay/MessageChain"
  },
  "license": "Apache-2.0",
  "icon": "assets/messagechain/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "me.ranzeplay.messagechain.client.MessageChainClient"
    ],
    "main": [
      "me.ranzeplay.messagechain.MessageChain"
    ]
  },
  "mixins": [
    "messagechain.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
